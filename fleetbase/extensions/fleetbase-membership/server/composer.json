{
    "name": "fleetbase/fleetbase-membership",
    "description": "Membership extension for Fleetbase - NFT-based member verification and profiles",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fleetbase Pte Ltd",
            "email": "hello@fleetbase.io"
        }
    ],
    "require": {
        "php": "^8.1",
        "fleetbase/core-api": "^1.0",
        "illuminate/database": "^10.0",
        "illuminate/support": "^10.0",
        "paragonie/sodium_compat": "^1.21"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Fleetbase\\Membership\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fleetbase\\Membership\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fleetbase\\Membership\\Providers\\MembershipServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
